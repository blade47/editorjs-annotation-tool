  "keywords": [
    "codex editor",
    "annotation-tool",
    "editor.js",
    "editorjs"
  ],
  "description": "Annotation Tool for Editor.js",
  "license": "MIT",
  "repository": "",
  "files": [
    "dist"
  ],
  "main": "./dist/annotation-tool.umd.js",
